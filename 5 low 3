n = int(input("Enter number of elements : "))
if n <= 0:
    print("Invalid input")
else:
    arr = list(map(int, input("Enter array elements : ").split()))
    if len(arr) != n:
        print("Invalid array")
    else:
        max_element = max(arr)
        min_element = min(arr)
        print("Largest element is : ", max_element)
        print("Smallest element is : ", min_element)
